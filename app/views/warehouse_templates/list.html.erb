<h1>
  Warehouse list
</h1>

<hr>

<table>
  <tr>


    <th>
      Name
    </th>

    <th>
      Address
      
    </th>

    <th>
      Neighborhood
    </th>

    <th>
      Max length sqft
    </th>

    <th>
      Max width sqft
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @warehouses.each do |warehouse| %>
  <tr>
   
    <td>
      <%= warehouse.name %>
    </td>

    <td>
      <%= warehouse.location %>
    </td>

    <td>
      <%= warehouse.neighborhood %>
    </td>

    <td>
      <%= warehouse.max_length_sqft %>
    </td>

    <td>
      <%= warehouse.max_width_sqft %>
    </td>


    <td>
      <%= time_ago_in_words(warehouse.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(warehouse.updated_at) %> ago
    </td>

    <td>
      <a href="/warehouses/<%= warehouse.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
